    "postinstall": "netlify-lambda install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "aws-sdk": "^2.733.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "convert-size": "^1.0.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "gatsby": "^2.23.4",
    "gatsby-cli": "^2.12.72",
    "gatsby-plugin-create-client-paths": "^2.3.4",
    "gatsby-plugin-netlify-cms": "^4.3.5",
    "gatsby-plugin-netlify-functions": "0.0.3",
    "gatsby-source-filesystem": "^2.3.11",
    "gatsby-transformer-json": "^2.4.5",
    "gatsby-transformer-remark": "^2.8.16",
